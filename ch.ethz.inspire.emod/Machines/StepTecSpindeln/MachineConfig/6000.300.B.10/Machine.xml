<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:machine xmlns:ns2="ch.ethz.inspire.emod">
    <machine>
        <machineComponent>
            <component xsi:type="spindle" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>StepTec-6000.300.B.10</type>
            </component>
            <name>Spindle</name>
        </machineComponent>
        <machineComponent>
            <component xsi:type="forcedFluidFlow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>GlykolWater_30</type>
            </component>
            <name>ForcedFluidFlow</name>
        </machineComponent>
    </machine>
    <simulators>
        <simController xsi:type="processSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>RotSpeed</name>
            <unit>
                <unitText>Hz</unitText>
            </unit>
        </simController>
        <simController xsi:type="processSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>Torque</name>
            <unit>
                <unitText>Nm</unitText>
            </unit>
        </simController>
        <simController xsi:type="constantSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>TemperatureIn</name>
            <unit>
                <unitText>K</unitText>
            </unit>
        </simController>
        <simController xsi:type="constantSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>PressureIn</name>
            <unit>
                <unitText>Pa</unitText>
            </unit>
        </simController>
        <simController xsi:type="staticSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>FlowRate</name>
            <unit>
                <unitText>mÂ³/s</unitText>
            </unit>
        </simController>
        <simController xsi:type="staticSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>State</name>
            <unit>
                <unitText>none</unitText>
            </unit>
        </simController>
    </simulators>
