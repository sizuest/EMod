<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:machine xmlns:ns2="ch.ethz.inspire.emod">
    <machine>
        <machineComponent>
            <component xsi:type="pumpPower" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>Example</type>
            </component>
            <name>PumpPower</name>
        </machineComponent>
        <machineComponent>
            <component xsi:type="servoMotor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>Example</type>
            </component>
            <name>ServoMotor</name>
        </machineComponent>
        <machineComponent>
            <component xsi:type="bearing" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>Example</type>
            </component>
            <name>Bearing</name>
        </machineComponent>
    </machine>
    <simulators>
        <simController xsi:type="staticSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>PumpCtrl</name>
            <unit>NONE</unit>
        </simController>
        <simController xsi:type="processSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>PumpMassFlow</name>
            <unit>KG_S</unit>
        </simController>
        <simController xsi:type="processSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>PumpTemperature</name>
            <unit>KELVIN</unit>
        </simController>
        <simController xsi:type="processSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>RotSpeed</name>
            <unit>RPM</unit>
        </simController>
        <simController xsi:type="processSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>Torque</name>
            <unit>NEWTONMETER</unit>
        </simController>
    </simulators>
    <connectionList/>
    <connectionList/>
    <connectionList/>
    <connectionList/>
    <connectionList/>
</ns2:machine>
