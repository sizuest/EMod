<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--
	Machine components definition file
	
	The <machine> contains a list of <machineComponent>s. Each
	<machineComponent> is specified by a <name>, a <component xsi:type> (e.g. linearMotor)
	and a <type>.
	the machine component objects are built from this list automatically.
-->
<ns2:machine xmlns:ns2="ch.ethz.inspire.emod.model">
    <machine>
        <machineComponent>
        	<!-- Comment on spindel -->
        	<name>spindel</name>
            <component xsi:type="linearMotor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>Siemens_1FE1115-6WT11</type>
            </component>
        </machineComponent>
        
        <machineComponent>
            <component xsi:type="linearMotor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>siemens123</type>
            </component>
            <name>x</name>
        </machineComponent>
        
        <machineComponent>
            <component xsi:type="linearMotor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>siemens123</type>
            </component>
            <name>y</name>
        </machineComponent>
        
        <machineComponent>
            <component xsi:type="constantComponent" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>80mmFan</type>
            </component>
            <name>Fan1</name>
        </machineComponent>
        
        <machineComponent>
        	<!-- Comment -->
            <component xsi:type="constantComponent" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>80mmFan</type>
            </component>
            <name>Fan2</name>
        </machineComponent>
    </machine>
</ns2:machine>
