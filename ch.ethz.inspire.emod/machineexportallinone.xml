<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:machine xmlns:ns2="ch.ethz.inspire.emod">
    <machine>
        <machineComponent>
            <component xsi:type="linearMotor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>Siemens_1FE1115-6WT11</type>
            </component>
            <name>spindel</name>
        </machineComponent>
        <machineComponent>
            <component xsi:type="linearMotor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>siemens123</type>
            </component>
            <name>x</name>
        </machineComponent>
        <machineComponent>
            <component xsi:type="linearMotor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>siemens123</type>
            </component>
            <name>y</name>
        </machineComponent>
        <machineComponent>
            <component xsi:type="constantComponent" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>80mmFan</type>
            </component>
            <name>Fan1</name>
        </machineComponent>
        <machineComponent>
            <component xsi:type="constantComponent" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
                <type>80mmFan</type>
            </component>
            <name>Fan2</name>
        </machineComponent>
    </machine>
    <simulators>
        <simController xsi:type="staticSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>spindelRPM</name>
            <unit>RPM</unit>
            <simulationPeriod>0.2</simulationPeriod>
        </simController>
        <simController xsi:type="geometricKienzleSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>spindelTorque</name>
            <unit>NEWTONMETER</unit>
            <simulationPeriod>0.2</simulationPeriod>
        </simController>
        <simController xsi:type="randomSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>xRPM</name>
            <unit>RPM</unit>
            <simulationPeriod>0.0</simulationPeriod>
        </simController>
        <simController xsi:type="randomSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>xTorque</name>
            <unit>NEWTONMETER</unit>
            <simulationPeriod>0.0</simulationPeriod>
        </simController>
        <simController xsi:type="randomSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>yRPM</name>
            <unit>RPM</unit>
            <simulationPeriod>0.0</simulationPeriod>
        </simController>
        <simController xsi:type="randomSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>yTorque</name>
            <unit>NEWTONMETER</unit>
            <simulationPeriod>0.0</simulationPeriod>
        </simController>
        <simController xsi:type="staticSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>test</name>
            <unit>NONE</unit>
            <simulationPeriod>0.2</simulationPeriod>
        </simController>
        <simController xsi:type="staticSimulationControl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <name>80mmFan</name>
            <unit>NONE</unit>
            <simulationPeriod>0.2</simulationPeriod>
        </simController>
    </simulators>
    <machineComponentList>
        <component xsi:type="linearMotor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <type>Siemens_1FE1115-6WT11</type>
        </component>
        <name>spindel</name>
    </machineComponentList>
    <machineComponentList>
        <component xsi:type="linearMotor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <type>siemens123</type>
        </component>
        <name>x</name>
    </machineComponentList>
    <machineComponentList>
        <component xsi:type="linearMotor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <type>siemens123</type>
        </component>
        <name>y</name>
    </machineComponentList>
    <machineComponentList>
        <component xsi:type="constantComponent" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <type>80mmFan</type>
        </component>
        <name>Fan1</name>
    </machineComponentList>
    <machineComponentList>
        <component xsi:type="constantComponent" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <type>80mmFan</type>
        </component>
        <name>Fan2</name>
    </machineComponentList>
</ns2:machine>
